{
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "marcin-orlowski/lombok-php": "^1.0",
    "doctrine/dbal": "^3.3",
    "doctrine/orm": "^2.11",
    "doctrine/migrations": "^3.5",
    "symfony/mailer": "^6.2",
    "symfony/cache": "^6.2",
    "twig/twig": "^3.4",
    "twig/intl-extra": "^3.4",
    "slim/slim": "4.*",
    "slim/psr7": "^1.6",
    "slim/twig-view": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
