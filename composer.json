{
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "marcin-orlowski/lombok-php": "^1.0",
    "php-di/php-di": "^6.4",
    "doctrine/dbal": "^3.4",
    "doctrine/orm": "2.13.4",
    "doctrine/migrations": "^3.5",
    "twig/twig": "^3.4",
    "twig/intl-extra": "^3.4",
    "slim/slim": "4.*",
    "slim/psr7": "^1.6",
    "slim/twig-view": "^3.3",
    "symfony/mailer": "^6.2",
    "symfony/cache": "^6.2",
    "symfony/console": "^6.2",
    "symfony/asset": "^6.2",
    "symfony/webpack-encore-bundle": "^1.16",
    "symfony/twig-bridge": "^6.2"

  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
