<table>
    <tr>
        <th>id</th>
        <th>email</th>
        <th>status</th>
    </tr>

    {% for user in users %}
        <tr>
            <td>{{ user.id }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.password_hash }}</td>
        </tr>
    {% else %}
        <tr>
            <td colspan="4">No user found</td>
        </tr>
    {% endfor %}
</table>